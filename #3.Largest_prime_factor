/*


The prime factors of 13195 are 5, 7, 13 and 29.

What is the largest prime factor of the number 600851475143 ?


*/

#include <iostream>

using namespace std;

int main()
{
    long long num = 600851475143;
    long long maxz(-1);
    int i(2);
    for(;;)
    {
        if(num%i==0)
        {
            if(maxz < i)
                maxz = i;
            num = num/i;
        }
        if(num==1)
        {
            break;
        }
        i++;
    }
    cout << maxz << endl;
}
